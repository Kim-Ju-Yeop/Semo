<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kr.hs.dgsw.stac.semo">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".widget.Application"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.view.AddActivity" />
        <activity android:name=".view.view.InfoActivity" />
        <activity android:name=".view.view.ListActivity" />
        <activity android:name=".view.view.SelectActivity" />
        <activity android:name=".view.view.SignUpActivity" />
        <activity android:name=".view.view.SignInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.view.SplashActivity" />
        <activity android:name=".view.view.CameraKitActivity"/>
        <activity android:name=".view.view.MainActivity"/>
    </application>
</manifest>